<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Basic Audio Input Permissions (works with ad-hoc signing) -->
    <key>com.apple.security.device.microphone</key>
    <true/>
    <key>com.apple.security.device.audio-input</key>
    <true/>
    
    <!-- Basic File Access (works with ad-hoc signing) -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>
    <key>com.apple.security.files.downloads.read-write</key>
    <true/>
    
    <!-- Network Access (works with ad-hoc signing) -->
    <key>com.apple.security.network.client</key>
    <true/>
    
    <!-- WARNING: DEVELOPMENT ONLY - DO NOT USE IN PRODUCTION -->
    <!-- These entitlements disable security features and should only be used for local testing -->
    
    <!-- Disable library validation for development with unsigned dylibs -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    
    <!-- Allow unsigned executable pages for development -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
</dict>
</plist>
